{
    "name": "divido/merchant-sdk-guzzle-6",
    "type": "library",
    "authors": [
        {
            "name": "Neil McGibbon",
            "email": "code@neilmcgibbon.com"
        },
        {
            "name": "Mike Lovely",
            "email": "mike.lovely@divido.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:dividohq/merchant-api-pub-sdk-php.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Divido\\MerchantSDKGuzzle6\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Divido\\MerchantSDKGuzzle6\\Test\\": "tests/"
        },
        "classmap": [
            "tests"
        ]
    },
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2",
        "divido/merchant-sdk": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "mockery/mockery": "1.1.0"
    }
}
